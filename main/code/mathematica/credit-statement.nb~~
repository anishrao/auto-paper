Cell[CellGroupData[{

Cell["EditToExportURL", "Section",
 CellChangeTimes->{{3.8244819497825937`*^9, 3.824481951984687*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"EditToExportURL", "[", "url_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"Append", "[", 
      RowBox[{
       RowBox[{"KeyDrop", "[", 
        RowBox[{"#", ",", 
         RowBox[{"{", 
          RowBox[{
          "\"\<Path\>\"", ",", "\"\<Query\>\"", ",", "\"\<Fragment\>\""}], 
          "}"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"\"\<Path\>\"", "\[Rule]", 
          RowBox[{"StringReplace", "[", 
           RowBox[{
            RowBox[{"#", "[", "\"\<Path\>\"", "]"}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"#", "[", "\"\<Path\>\"", "]"}], "\[LeftDoubleBracket]", 
              RowBox[{"-", "1"}], "\[RightDoubleBracket]"}], "\[Rule]", 
             "\"\<export\>\""}]}], "]"}]}], ",", 
         RowBox[{"\"\<Query\>\"", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{
               "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
               "\[Rule]", 
               RowBox[{
               "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
              "&"}], "@", 
             RowBox[{"StringSplit", "[", 
              RowBox[{
               RowBox[{"parsedURL", "[", "\"\<Fragment\>\"", "]"}], ",", 
               "\"\<=\>\""}], "]"}]}], ",", 
            RowBox[{"\"\<format\>\"", "\[Rule]", "\"\<csv\>\""}]}], "}"}]}], 
         ",", 
         RowBox[{"\"\<Fragment\>\"", "\[Rule]", "None"}]}], "}"}]}], "]"}], 
     "&"}], "@", 
    RowBox[{"URLParse", "@", "url"}]}], "//", 
   "URLBuild"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"EditToExportURL", "::", "usage"}], "=", 
   "\"\<EditToExportURL[url] converts a Google Sheets spreadsheet edit URL \
into an export (i.e. download) URL by breaking the URLs into parts via \
URLParse, replacing values in the Association, and building it back up via \
URLBuild.\>\""}], ";"}]}], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.8244801050031905`*^9, 3.8244801089875383`*^9}, {
   3.824480314392973*^9, 3.824480403970791*^9}, {3.824480454829897*^9, 
   3.8244805659280863`*^9}, {3.824480602211356*^9, 3.8244806390902386`*^9}, {
   3.824480671520426*^9, 3.824480723995835*^9}, {3.8244807717362795`*^9, 
